<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Price Tracking</title>
</head>

<style>
    .prod_single_container {
        border: 1px solid;
        
    }
</style>

<body>
    <h3>Price History</h3>
    <ul>
    <% products.forEach((single) => {
        %> <li> <div class="prod_single_container">
            <h2><%= single.name %></h2>
            <p> <%= single.price_history[0].price %> </p>

        </div> </li> <%
    }); %>
    </ul>
</body>
</html>